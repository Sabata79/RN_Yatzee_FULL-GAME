{
  "expo": {
    "sdkVersion": "53.0.0",
    "name": "Yatzy",
    "slug": "SimpleYatzee",
    "version": "2.1.9",
    "orientation": "portrait",
    "icon": "./assets/desktopIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c3e50"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": "2.1.9",
      "buildNumber": "2.1.9",
      "bundleIdentifier": "com.SimpleYatzee"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/desktopIcon.png",
        "backgroundColor": "#2c3e50"
      },
      "package": "com.SimpleYatzee",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK",
        "com.google.android.gms.permission.AD_ID"
      ],
      "runtimeVersion": "2.1.9",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "sabata_79",
    "extra": {
      "eas": {
        "projectId": "d8c9a220-7647-4af7-906f-8ce3856b7189"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "kotlinVersion": "2.0.21"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#2c3e50",
          "image": "./assets/splash.png"
        }
      ]
    ],
    "updates": {
      "url": "https://u.expo.dev/d8c9a220-7647-4af7-906f-8ce3856b7189",
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-9474518865389369~6133058884"
  }
}
